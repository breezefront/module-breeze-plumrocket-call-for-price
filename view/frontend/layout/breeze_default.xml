<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="breeze.js">
            <arguments>
                <argument name="bundles" xsi:type="array">
                    <item name="dynamic" xsi:type="array">
                        <item name="items" xsi:type="array">
                            <item name="Plumrocket_CallForPrice/js/popup" xsi:type="array">
                                <item name="path" xsi:type="string">Plumrocket_CallForPrice/js/popup</item>
                            </item>
                            <item name="Plumrocket_CallForPrice/js/optionProvider" xsi:type="array">
                                <item name="path" xsi:type="string">Plumrocket_CallForPrice/js/optionProvider</item>
                            </item>
                            <item name="Plumrocket_CallForPrice/js/reCaptcha" xsi:type="array">
                                <item name="path" xsi:type="string">Plumrocket_CallForPrice/js/reCaptcha</item>
                            </item>
                        </item>
                    </item>
                </argument>
            </arguments>
        </referenceBlock>
    </body>
</page>
